          <field name = "model">res.partner</field>
          <field name = "inherit_id" ref ="base.view_partner_form" >res.partner</field>
          <field eval = "1" name ="priority" ></field>
          <field name = "arch" type="xml" ></field>
              <data>
                  <xpath expr="//field[@name='mobile']" position="after">
                      <field name="birthday"/>
                  </xpath>
              </data>
      </record>
  </data>
</odoo>